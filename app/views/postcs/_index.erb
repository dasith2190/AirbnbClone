<h1>Listing Postcs</h1>
<table>
  <tbody>
  <tr>
    <th>Post title</th>
    <th>Post description</th>
    <th>Address</th>
    <th>Country</th>
    <th>Zip</th>
    <th>City</th>
    <th>Price</th>
    <th>Home type</th>
    <th>Rooms</th>
    <th>Baths</th>
    <th>Price</th>
    <th>Price type</th>
    <th colspan="3"></th>
  </tr>

  <% @postcs.each do |postc| %>
      <tr>
        <td><%= postc.post_title %></td>
        <td><%= postc.post_description %></td>
        <td><%= postc.address %></td>
        <td><%= postc.country %></td>
        <td><%= postc.zip %></td>
        <td><%= postc.city %></td>
        <td><%= postc.price %></td>
        <td><%= postc.home_type %></td>
        <td><%= postc.rooms %></td>
        <td><%= postc.baths %></td>
        <td><%= postc.price %></td>
        <td><%= postc.price_type %></td>
        <td><%= link_to 'Show', postc %></td>
        <td><%= link_to 'Edit', edit_postc_path(postc) %></td>
        <td><%= link_to 'Destroy', postc, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Postc', new_postc_path %>
